lse, "user": {"id": "726999", "name": "Aaron O'Leary", "username": "aaronoleary", "avatarUrl": "https://ph-avatars.imgix.net/726999/17085108-59fb-4cb7-8d9e-91a0c10cc14f.jpeg", "__typename": "User", "isAccountVerified": true, "isAmbassador": false, "selectedBylineProduct": {"id": "112572", "name": "Product Hunt", "slug": "producthunt", "logoUuid": "d618ea86-70ae-4d12-8ec2-0ed78c723ddf.png", "isNoLongerOnline": false, "__typename": "Product"}, "topProductBadge": null, "topHunterBadge": null, "topLaunchBadge": null}, "hasVoted": false, "votesCount": 257, "__typename": "DiscussionThread", "commentsCount": 25, "featuredComment": null, "primaryForum": {"id": "48762", "slug": "producthunt", "subject": {"id": "112572", "name": "Product Hunt", "logoUuid": "d618ea86-70ae-4d12-8ec2-0ed78c723ddf.png", "__typename": "Product"}, "__typename": "DiscussionForumType"}}, "__typename": "DiscussionThreadEdge"}, {"node": {"id": "795032", "pageViewsCount": 201, "slug": "we-are-launching-typeless-for-ios-ask-me-anything", "createdAt": "2025-12-22T23:39:28-08:00", "path": "/p/typeless-2/we-are-launching-typeless-for-ios-ask-me-anything", "title": "We are launching Typeless for iOS, ask me anything!", "descriptionPreview": "<p>Hey Product Hunt community! </p><p></p><p>This is YUKI from the Typeless marketing team.</p><p>Let's be honest: mobile typing is broken. We lose ideas because typing feels like too much work. We delay replies because it's tedious. We fight autocorrect, fix typos, and hope our messages don't sound rushed.</p>", "featuredAt": null, "pinned": false, "user": {"id": "8872240", "name": "YUKI KE", "username": "yuki1028", "avatarUrl": "https://ph-avatars.imgix.net/8872240/b32262ff-09e2-42fd-82b2-8fa59ca50812.png", "__typename": "User", "isAccountVerified": false, "isAmbassador": false, "selectedBylineProduct": {"id": "1126415", "name": "Typeless", "slug": "typeless-2", "logoUuid": "e767738e-d2a4-4479-8e28-41dc889b32d6.png", "isNoLongerOnline": false, "__typename": "Product"}, "topProductBadge": null, "topHunterBadge": null, "topLaunchBadge": null}, "hasVoted": false, "votesCount": 22, "__typename": "DiscussionThread", "commentsCount": 8, "featuredComment": null, "primaryForum": {"id": "547245", "slug": "typeless-2", "subject": {"id": "1126415", "name": "Typeless", "logoUuid": "e767738e-d2a4-4479-8e28-41dc889b32d6.png", "__typename": "Product"}, "__typename": "DiscussionForumType"}}, "__typename": "DiscussionThreadEdge"}, {"node": {"id": "663054", "pageViewsCount": 54, "slug": "can-anyone-recommend-an-ai-audio-survey-tool", "createdAt": "2025-02-26T08:05:44-08:00", "path": "/p/general/can-anyone-recommend-an-ai-audio-survey-tool", "title": "Can anyone recommend an AI audio survey tool?", "descriptionPreview": "<p>I'm looking for tools to help me brief clients and get their answers in voice format. Recently, I found @Yapz It really impressed me. Simple to use, the delightful format of briefing people, only a positive experience. The only downside is that I cannot see the whole call transcript, which is crucial for me. Are there any free/cheap AI alternatives that makes summaries of answers and saves transcripts at the same time?</p>", "featuredAt": null, "pinned": false, "user": {"id": "4162687", "name": "Alex Robeiko", "username": "alex_robeiko", "avatarUrl": "https://ph-avatars.imgix.net/4162687/original.jpeg", "__typename": "User", "isAccountVerified": false, "isAmbassador": false, "selectedBylineProduct": null, "topProductBadge": null, "topHunterBadge": null, "topLaunchBadge": null}, "hasVoted": false, "votesCount": 11, "__typename": "DiscussionThread", "commentsCount": 5, "featuredComment": null, "primaryForum": {"id": "7", "slug": "general", "subject": {"id": "100", "name": "General", "thumbnailUuid": "85c8f053-f68f-486f-a83b-79bd21080edc.svg", "__typename": "DiscussionCategory"}, "__typename": "DiscussionForumType"}}, "__typename": "DiscussionThreadEdge"}, {"node": {"id": "727483", "pageViewsCount": 1712, "slug": "aqua-voice-vs-wispr-flow", "createdAt": "2025-07-24T07:11:55-07:00", "path": "/p/aqua/aqua-voice-vs-wispr-flow", "title": "Aqua Voice vs Wispr Flow", "descriptionPreview": "<p>I'm a big fan of voice dictation apps. In fact, I'm using one right now to write this very post (you'll have to wait till the end to see which one I'm using )</p><p></p><p>The two main products I've used in this space are @Aqua Voice and @Wispr Flow. From talking to others, these are the two that I typically hear people mention using. In general, I hear a lot more people talk about using Wispr Flow.</p><p></p>", "featuredAt": null, "pinned": false, "user": {"id": "294167", "name": "Jake Crump", "username": "jakecrump", "avatarUrl": "https://ph-avatars.imgix.net/294167/original.jpeg", "__typename": "User", "isAccountVerified": true, "isAmbassador": false, "selectedBylineProduct": {"id": "112572", "name": "Product Hunt", "slug": "producthunt", "logoUuid": "d618ea86-70ae-4d12-8ec2-0ed78c723ddf.png", "isNoLongerOnline": false, "__typename": "Product"}, "topProductBadge": null, "topHunterBadge": null, "topLaunchBadge": null}, "hasVoted": false, "votesCount": 27, "__typename": "DiscussionThread", "commentsCount": 14, "featuredComment": null, "primaryForum": {"id": "473553", "slug": "aqua", "subject": {"id": "1054519", "name": "Aqua Voice", "logoUuid": "fb2279a2-8a40-4ae3-bb8d-67f7559df588.png", "__typename": "Product"}, "__typename": "DiscussionForumType"}}, "__typename": "DiscussionThreadEdge"}], "pageInfo": {"hasNextPage": true, "__typename": "PageInfo"}, "__typename": "DiscussionThreadConnection"}, "relevantReviews": {"edges": [], "__typename": "ProductCategoryRelevantReviewConnection"}}}}